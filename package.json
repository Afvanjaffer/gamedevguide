  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "develop": "gatsby clean && gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .eslintignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "lint:mdwip": "remark --no-color --rc-path .remarkrc contents/guides/ue4guide/wip/**/*.md > lintlog.txt 2>&1",
    "format:md": "remark --rc-path .remarkrc contents/guides/**/*.md -o",
    "format:mdx": "remark --rc-path .remarkrc contents/guides/**/*.mdx -o",
    "format:js": "prettier --write \"**/*.js\"",
    "formatp:mdwip": "prettier --parser markdown --write \"contents/guides/ue4guide/wip/**/*.md\"",
    "formatp:md": "prettier --parser markdown --write \"contents/guides/**/*.md\"",
    "formatp:mdx": "prettier --parser mdx --write \"contents/**/*.mdx\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "pushd ..\\..\\knl\\knl-gamedevguide & git pull & git push & git fetch & popd"
  },
